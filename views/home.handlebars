<div>
  {{#each postMap}}
    <div class="card border border-dark p-2 m-2">
      <div class="card-header">
        {{title}}
        by
        {{user.username}}
      </div>
      <div class="card-body">
        <blockquote class="blockquote mb-0">
          <p>{{content}}</p>
        </blockquote>
      </div>
    </div>
  {{/each}}
</div>